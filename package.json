{
  "name": "c-level-hire-ai-agent",
  "version": "2.0.0",
  "description": "Democratizing executive job search - 0.1% of target salary per week",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "node scripts/init-db.js && node dist/index.js",
    "startup": "node startup.js",
    "migrate": "ts-node src/database/migrate.ts",
    "test": "jest",
    "deploy": "railway up"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "openai": "^4.24.1",
    "redis": "^4.6.10",
    "socket.io": "^4.6.0",
    "sqlite3": "^5.1.7",
    "stripe": "^14.5.0",
    "typescript": "^5.0.4",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.17",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  }
